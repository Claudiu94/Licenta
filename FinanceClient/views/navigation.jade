link(rel='stylesheet', href='/stylesheets/navigation.css', type='text/css')
script(type='application/javascript', src='/javascripts/navigation.js')
- local_data = JSON.stringify(session)
		input(type='hidden', value=local_data)#myLocalDataObj
		nav
			ul(style='display: inline-block; vertical-align: middle; margin: 0px;')
				li(style='display: inline-block; margin-right: 10px')
					a#account.nav-link(href='account')
						| Sign In 
				li(style='display: inline-block; margin-right: 10px')
					a.nav-link(href='') Link
		section#body.width
			aside#sidebar.column-left
				header
					h1
						a(href='#')
							span(style='font-size: 63px; display: block;') Finance
							| Analyzer
					- if (typeof session.username !== 'undefined')  
						h2 Welcome to my website,  #{session.username}!
					- else
						h2 Welcome to my website!
				nav#mainnav
					ul
						li#home.selected-item
							a(href='/home') Home
						li#portofolio
							a(href='#') Portofolio
						li#details
							a(href='details') Apple Test
						li
							a(href='#') News
						li
							a(href='#') Contact